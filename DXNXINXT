local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("By.DXNXINXT(Demo)", "Midnight")
local Tab = Window:NewTab("ทั่วไป")
Section:NewButton("ประหารชีวิต", "ButtonInfo", function()
    print("Clicked")
    local replicatedStorage = game:GetService("ReplicatedStorage")
local players = game:GetService("Players")

local killAllEvent = replicatedStorage:FindFirstChild("KillAllEvent")

if killAllEvent then
    killAllEvent.OnServerEvent:Connect(function(player)
        -- ฆ่าผู้เล่นทุกคนในเกม
        for _, target in pairs(players:GetPlayers()) do
            if target.Character and target.Character:FindFirstChild("Humanoid") then
                target.Character.Humanoid.Health = 0
            end
        end
        print(player.Name .. " ได้ใช้คำสั่ง Kill All!")
    end)
end
end)
local Section = Tab:NewSection("function")
    Section:NewButton("ฆ่าไม่ตาย", "ButtonInfo", function()
        print("Clicked")
        local player = game.Players.LocalPlayer 
local character = player.Character or player.CharacterAdded:Wait() 

local function makeImmortal()
    local humanoid = character:FindFirstChild("Humanoid")
    if humanoid then
        humanoid.HealthChanged:Connect(function()
            if humanoid.Health <= 0 then
                humanoid.Health = humanoid.MaxHealth 
            end
        end)
    end
end

if character then
    makeImmortal()
else
    player.CharacterAdded:Connect(function()
        character = player.Character
        makeImmortal()
    end)
end
    end)
Section:NewButton("สั่งฆ่า", "ButtonInfo", function()
    print("Clicked")
        end)
Section:NewButton("ยายสปี๊ด", "ButtonInfo", function()
            print("Clicked")
            game.Players.LocalPlayer.character.Humanoid.WalkSpeed = 10000
        end)
Section:NewButton("โดดสูง", "ButtonInfo", function()
            print("Clicked")
            game.Players.LocalPlayer.character.Humanoid.JumpPower = 500
        end)
local Tab = Window:NewTab("SeeAll")
local Section = Tab:NewSection("function")       
    Section:NewButton("มองหาศัตรู", "ButtonInfo", function()
        print("Clicked")
        local Players = game:GetService("Players")

for _, p in pairs(Players:GetPlayers()) do
    if p.Character then
        local h = Instance.new("Highlight", p.Character)
        h.FillColor = Color3.new(1, 0, 0) 
    end
end

Players.PlayerAdded:Connect(function(p)
    p.CharacterAdded:Connect(function(c)
        Instance.new("Highlight", c).FillColor = Color3.new(1, 0, 0)
    end)
end)
    end)local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
